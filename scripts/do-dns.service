[Unit]
Description=do-dns
After=network.target

[Service]
ExecStart=/opt/illpro/do-dns/src/main.js
Restart=always
User=nobody
Group=nogroup
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
Environment=DNS_DOMAIN=
Environment=DNS_NAME=
WorkingDirectory=/opt/illpro/do-dns/src/main.js

[Install]
WantedBy=multi-user.target
